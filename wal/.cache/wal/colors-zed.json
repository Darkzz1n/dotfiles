{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0a081390",
                "editor.background": "#0a081390",
                "editor.foreground": "#c1c1c4",
                "text": "#c1c1c4",
                "text.muted": "#c1c1c470",
                "text.ignored": "#c1c1c440",
                "text.placeholder": "#c1c1c450",
                "ignored": "#c1c1c430",
                "element.hover": "#c1c1c430",
                "ghost_element.hover": "#8b1d4830",
                "ghost_element.selected": "#8b1d4830",
                "ghost_element.active": "#8b1d4860",
                "border": "#0a0813",
                "editor.highlighted_line.background": "#8b1d4810",
                "editor.active_line.background": "#8b1d4810",
                "panel.background": "#0a081390",
                "title_bar.background": "#0a081390",
                "title_bar.inactive_background": "#0a081390",
                "status_bar.background": "#0a081390",
                "drop_target.background": "#0a081390",
                "elevated_surface.background": "#0a0813",
                "toolbar.background": "#0a081390",
                "tab_bar.background": "#0a081390",
                "tab.inactive_background": "#0a081390",
                "tab.active_background": "#8b1d4830",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0a0813",
                "scrollbar.thumb.background": "#8b1d48",
                "editor.gutter.background": "#0a081390",
                "terminal.background": "#0a081310",
                "terminal.foreground": "#c1c1c4",
                "terminal.dim_foreground": "#c1c1c4",
                "terminal.bright_foreground": "#c1c1c4",
                "terminal.ansi.black": "#0a0813",
                "terminal.ansi.red": "#8b1d48",
                "terminal.ansi.green": "#504e7e",
                "terminal.ansi.yellow": "#8f3c85",
                "terminal.ansi.blue": "#77517e",
                "terminal.ansi.magenta": "#516984",
                "terminal.ansi.cyan": "#89758a",
                "terminal.ansi.white": "#8e8c96",
                "terminal.ansi.bright_black": "#5a576b",
                "terminal.ansi.bright_red": "#BA2760",
                "terminal.ansi.bright_green": "#6B68A9",
                "terminal.ansi.bright_yellow": "#BF50B2",
                "terminal.ansi.bright_blue": "#9F6CA9",
                "terminal.ansi.bright_magenta": "#6C8CB1",
                "terminal.ansi.bright_cyan": "#B79CB8",
                "terminal.ansi.bright_white": "#c1c1c4",
                "modified": "#8b1d48",
                "syntax": {
                    "attribute": {
                        "color": "#8b1d48"
                    },
                    "boolean": {
                      "color": "#504e7e"
                    },
                    "comment": {
                      "color": "#5a576b"
                    },
                    "comment.doc": {
                      "color": "#5a576b"
                    },
                    "constant": {
                      "color": "#6B68A9"
                    },
                    "function": {
                      "color": "#B79CB8"
                    },
                    "keyword": {
                      "color": "#77517e"
                    },
                    "number": {
                      "color": "#516984"
                    },
                    "operator": {
                      "color": "#77517e"
                    },
                    "string": {
                      "color": "#8b1d48"
                    },
                    "variable": {
                      "color": "#504e7e"
                    }
                },
                "players": [
                  {
                    "cursor": "#8b1d48",
                    "background": "#0a0813",
                    "selection": "#8b1d4830"
                  }
                ]
            }
        }
    ]
}
